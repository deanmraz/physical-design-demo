<button {{action "shuffle"}}>Shuffle</button>

<div class="speaker-icons">
  {{#liquid-with model as |currentModel|}}
  {{#each currentModel as |speaker|}}
    {{#link-to "speaker" speaker}}
      <img class="speaker-image-small" alt="{{speaker.name}}" src="{{speaker.imageURL}}" data-speaker-id={{speaker.id}} >
    {{/link-to}}
  {{/each}}
  {{/liquid-with}}
</div>